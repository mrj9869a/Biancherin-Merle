<h1>Listing model_names</h1>

<table>
  <thead>
    <tr>
      <th>Element</th>
      <th>Etc</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @model_names.each do |model_name| %>
      <tr>
        <td><%= model_name.element %></td>
        <td><%= model_name.etc %></td>
        <td><%= link_to 'Show', model_name %></td>
        <td><%= link_to 'Edit', edit_model_name_path(model_name) %></td>
        <td><%= link_to 'Destroy', model_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Model name', new_model_name_path %>
